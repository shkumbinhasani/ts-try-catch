{
  "name": "@shkumbinhsn/try-catch-eslint",
  "version": "0.0.3",
  "description": "ESLint plugin to enforce try-catch patterns with @shkumbinhsn/try-catch",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "typescript",
    "try-catch",
    "error-handling"
  ],
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "typescript-eslint": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "eslint": "^9.0.0",
    "vitest": "^1.2.0"
  },
  "author": "Shkumbin Hasani",
  "license": "MIT"
}
